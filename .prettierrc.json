{
  "singleQuote": true,
  "semi": false,
  "printWidth": 100,
  "tabWidth": 2,
  "trailingComma": "all",
  "plugins": ["prettier-plugin-css-order", "@trivago/prettier-plugin-sort-imports"],
  "importOrder": ["vue", "^@/components/(.*)$", "^@/utils/(.*)$", "^[./]"],
  "importOrderSortSpecifiers": true,
  "importOrderSeparation": true
}
